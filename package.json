{
  "name": "stencil-redux-starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil-Redux App Starter",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npx stencil build --prerender",
    "dev": "npx concurrently \"npx stencil build --dev --watch\" \"npx stencil-dev-server\" ",
    "serve": "npx stencil-dev-server",
    "start": "npm run dev",
    "test": "npx jest --no-cache",
    "test.watch": "npx jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "0.7.7",
    "@stencil/redux": "0.0.4",
    "@stencil/router": "0.0.28",
    "csx": "^8.5.0",
    "express": "^4.16.3",
    "lodash.get": "^4.4.2",
    "lodash.map": "^4.6.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "typescript": "^2.9.0-dev.20180330",
    "typestyle": "^1.7.2"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "jest-cli": "^22.4.3"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "collectCoverage": true,
    "verbose": true,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
